{
  "name": "qa-rdc-template-webpack",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config scripts/webpack.dev.config.js",
    "dev:all": "concurrently \"cd ../remoteComponent && pnpm dev\" \"pnpm dev\"",
    "build:prod": "cross-env NODE_ENV=production webpack --config scripts/webpack.prod.config.js",
    "build:staging": "cross-env NODE_ENV=staging webpack --config scripts/webpack.prod.config.js",
    "build:test": "cross-env NODE_ENV=test webpack --config scripts/webpack.prod.config.js",
    "deploy:prod": "pnpm build:prod && webstatic publish --appkey=com.sankuai.waimaiqafc.aie --env=prod --artifact=dist --build-command='pnpm build:prod' --token=98f9aed6-a075-461f-b6da-7ca4598c3829",
    "deploy:staging": "pnpm build:staging && webstatic publish --appkey=com.sankuai.waimaiqafc.aie --env=staging --artifact=dist --build-command='pnpm build:staging' --token=98f9aed6-a075-461f-b6da-7ca4598c3829",
    "deploy:test": "pnpm build:test && webstatic publish --appkey=com.sankuai.waimaiqafc.aie --env=test --artifact=dist --build-command='pnpm build:test' --token=269883ad-b7b0-4431-b5e7-5886cd1d590f"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "antd": "5.22.2",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "typescript": "5.7.2",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^6.0.1"
  }
